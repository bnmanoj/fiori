{
  "_version": "1.28.0",
  "sap.app": {
    "id": "com.sap.s.zstudent",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.0",
      "toolsId": "06c21818-4885-416f-834e-c4c6b42c47b2"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.84.48",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.s.zstudent.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.sap.s.zstudent.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Routestudent",
          "pattern": ":?query:",
          "target": [
            "Targetstudent"
          ]
        },
        {
          "name": "Routedetails",
          "pattern": "Routedetails/{sonumber}",
          "target": [
            "Targetdetails"
          ]
        },
        {
          "name": "Routedata",
          "pattern": "Routedata",
          "target": [
            "Targetdata"
          ]
        }
      ],
      "targets": {
        "Targetstudent": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "student",
          "viewName": "student"
        },
        "Targetdetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "details",
          "viewName": "details"
        },
        "Targetdata": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "data",
          "viewName": "data"
        }
      }
    },
    "rootView": {
      "viewName": "com.sap.s.zstudent.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
